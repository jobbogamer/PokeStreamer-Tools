{
    "sourceMaps": true,
    "presets": [
        "env",
        "es2017",
        "es2016",
        [ "es2015", { "modules" : "commonjs" } ]
    ],
    "env": {
        "test": {
            "plugins": [
                "rewire"
            ]
        }
    },
    "plugins": [
        [
            "transform-runtime",
            {
                "polyfill": false
            }
        ]
    ],
    "retainLines": true
}